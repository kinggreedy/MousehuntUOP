{
	"name": "Đây là xe hơm thắng",
	"description": "Hơm có thắng hơm phải vì xe hơm có thắng....Hơm có thắng là để tới King's Stockade nhanh hơn :yaoming:",
	"version": "3.1.3",
	"update_url": "http://kinggreedy.azurewebsites.net/UOP/updatesVIP.xml",
	"manifest_version": 2,
	"icons": { "128": "icon.jpg"},
	
	"permissions": [
		"app",
		"tabs",
		//"storage",
		"webRequest",
		"webRequestBlocking",
		"*://*/*",
		//"*://www.mousehuntgame.com/*",
		//"*://apps.facebook.com/mousehunt/*",
		"file://*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [{
		"js": ["/resources/jquery.min.js","/resources/jbclock/jbclock.js","/resources/luxCountdown/luxUtilities.min.js","/resources/luxCountdown/luxCountdown.min.js","/resources/luxCountdown/luxCountdownExpand.min.js","content.js"],
		"matches": [ "*://www.mousehuntgame.com/*", "https://apps.facebook.com/mousehunt/*" ],
		"all_frames" : true,
		"run_at": "document_start"
	}],
	"web_accessible_resources": [
		"css/*",
		"images/*",
		"resources/*"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
